<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hello.shopping_war.member.repository.MemberMapper">
<select id="loginfMemberInfo" resultType="Customer">
    select *
    from customer
    where customer_id = #{customerId}
    and password=#{password}
</select>
</mapper>